<?xml version="1.0"?>
<launch>

    <arg name="gzclient" default="true"/>

    <include file="$(find ow_fps_monitor)/launch/europa_terminator_workspace.launch" />

    <test test-name="minimum_fps" pkg="ow_fps_monitor" type="test_minimum_fps.py">
        <param name="min_fps" value="30.0"/>
        <param name="warmup_period" value="10.0" />  <!-- how much to wait before observing avg fps -->
        <param name="test_duration" value="20.0"/>  <!-- total test duration, must be larger than warmup period -->
    </test>

</launch>
